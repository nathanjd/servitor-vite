{
  "name": "servitor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "yarn build && yarn upload && yarn invalidate-cache",
    "invalidate-cache": "aws cloudfront create-invalidation --distribution-id E3EICN0K00SFIW --paths '/*'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "upload": "aws s3 cp dist/ s3://servitor-pwa.com --recursive"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.7",
    "imurmurhash": "^0.1.4",
    "json-stable-stringify": "^1.1.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/imurmurhash": "^0.1.4",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^14.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-pwa": "^0.19.6",
    "vitest": "^1.4.0"
  }
}
